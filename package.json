{
  "name": "nestjs-auth-starter",
  "version": "1.0.0",
  "description": "Nestjs starter with authentication boilerplate",
  "main": "build/main.js",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prebuild": "rimraf build",
    "build": "swc ./src -Ds -d build",
    "start": "node build/main.js",
    "lint": "eslint ./src --ext .ts",
    "lint:fix": "npm run lint -- --fix",
    "lint-staged": "lint-staged",
    "test": "jest",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oscaroox/nestjs-auth-starter.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Myreage/nestjs-auth-starter/issues"
  },
  "homepage": "https://github.com/Myreage/nestjs-auth-starter#readme",
  "devDependencies": {
    "@faker-js/faker": "^7.5.0",
    "@nestjs/testing": "^9.1.2",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.4",
    "@swc/jest": "^0.2.23",
    "@types/bull": "^3.15.9",
    "@types/email-templates": "^10.0.1",
    "@types/jest": "^29.1.1",
    "@types/mjml": "^4.7.0",
    "@types/node": "^18.8.0",
    "@types/nodemailer": "^6.4.6",
    "@types/nodemailer-mailgun-transport": "^1.4.3",
    "@types/passport-jwt": "^3.0.7",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "chokidar": "^3.5.3",
    "concurrently": "^7.4.0",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.1",
    "jest": "^29.1.2",
    "lint-staged": "^13.0.3",
    "nodemon": "^2.0.20",
    "pino-pretty": "^9.1.0",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "supertest": "^6.3.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
